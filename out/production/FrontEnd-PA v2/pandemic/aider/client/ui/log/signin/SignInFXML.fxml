<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pandemic.aider.client.ui.log.signin.SignInController">
    <children>
        <AnchorPane layoutX="236.0" layoutY="88.0" prefHeight="600.0" prefWidth="900.0" AnchorPane.bottomAnchor="0.0"
                    AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox layoutX="313.0" layoutY="179.0" prefHeight="242.0" prefWidth="273.0">
                    <children>
                        <HBox>
                            <children>
                                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                    <image>
                                        <Image url="@../../../res/icons8-user-24-black.png"/>
                                    </image>
                                </ImageView>
                                <TextField fx:id="usernameTextField" prefWidth="218.0" promptText="Enter Username">
                                    <HBox.margin>
                                        <Insets bottom="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </TextField>
                            </children>
                        </HBox>
                        <HBox>
                            <children>
                                <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../../../res/icons8-password-30-black.png"/>
                                    </image>
                                    <HBox.margin>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                    </HBox.margin>
                                </ImageView>
                                <StackPane>
                                    <children>
                                        <TextField fx:id="passwordTextField" prefHeight="26.0" prefWidth="230.0"
                                                   promptText="Enter Password">
                                            <StackPane.margin>
                                                <Insets bottom="10.0" right="10.0" top="10.0"/>
                                            </StackPane.margin>
                                        </TextField>
                                        <PasswordField fx:id="passwordHiddenField" prefHeight="24.0" prefWidth="200.0"
                                                       promptText="Enter Password">
                                            <StackPane.margin>
                                                <Insets bottom="10.0" right="10.0" top="10.0"/>
                                            </StackPane.margin>
                                        </PasswordField>
                                    </children>
                                </StackPane>
                            </children>
                        </HBox>
                        <CheckBox fx:id="passwordCheckBoxToggle" mnemonicParsing="false" onAction="#showPassword"
                                  text="Show Password">
                            <VBox.margin>
                                <Insets bottom="10.0" left="44.0" right="10.0" top="5.0"/>
                            </VBox.margin>
                        </CheckBox>
                        <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-alignment: center;">
                            <children>
                                <Button fx:id="sigInButton" mnemonicParsing="false" prefHeight="24.0" prefWidth="80.0"
                                        text="Sign In">
                                    <VBox.margin>
                                        <Insets bottom="10.0" top="10.0"/>
                                    </VBox.margin>
                                </Button>
                                <Separator prefHeight="6.0" prefWidth="276.0"/>
                                <Button fx:id="signUpButton" mnemonicParsing="false" prefHeight="24.0" prefWidth="80.0"
                                        text="Sign Up">
                                    <VBox.margin>
                                        <Insets bottom="10.0" top="10.0"/>
                                    </VBox.margin>
                                </Button>
                            </children>
                        </VBox>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </children>
</AnchorPane>
