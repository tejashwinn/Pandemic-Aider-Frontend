<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import java.lang.String?>
<AnchorPane maxHeight="-Infinity"
            maxWidth="-Infinity" prefHeight="70.0"
            prefWidth="760.0"
            style="-fx-border-color: #121212; -fx-background-radius: 30; -fx-border-radius: 30; -fx-background-color: #252525;"
            styleClass="user-pane-hover"
            stylesheets="@../../style/UserPane.css"
            xmlns="http://javafx.com/javafx/16"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pandemic.aider.client.ui.main.UserPaneController">
	<children>
		<Button maxHeight="-Infinity"
		        maxWidth="-Infinity"
		        minHeight="-Infinity"
		        mnemonicParsing="false"
		        onAction="#viewUser"
		        prefHeight="70.0"
		        prefWidth="760.0"
		        style="-fx-border-color: #121212; -fx-background-radius: 30; -fx-border-radius: 30; -fx-background-color: #252525;"
		        styleClass="user-pane-hover"
		        stylesheets="@../../style/UserPane.css">
			<graphic>
				<VBox style="-fx-background-color: #252525; -fx-background-radius: 20; -fx-border-color: #252525; -fx-border-radius: 30;"
				      stylesheets="@../../style/UserPane.css">
					<children>
						<Label fx:id="nameLabel"
						       maxHeight="1.7976931348623157E308"
						       maxWidth="1.7976931348623157E308"
						       prefHeight="38.0"
						       prefWidth="654.0"
						       style="-fx-border-radius: 30; -fx-background-radius: 30;"
						       styleClass="user-pane"
						       stylesheets="@../../style/UserPane.css"
						       text="Username"
						       textFill="WHITE">
							<font>
								<Font name="System Bold Italic"
								      size="18.0"/>
							</font>
							<padding>
								<Insets bottom="5.0"
								        left="5.0"
								        right="5.0"
								        top="5.0"/>
							</padding>
						</Label>
						<Label fx:id="usernameLabel"
						       maxHeight="1.7976931348623157E308"
						       maxWidth="1.7976931348623157E308"
						       prefHeight="20.0"
						       prefWidth="730.0"
						       style="-fx-border-radius: 30; -fx-background-radius: 30;"
						       styleClass="user-pane"
						       stylesheets="@../../style/UserPane.css"
						       text="Name"
						       textFill="WHITE">
							<font>
								<Font name="System Bold Italic"
								      size="12.0"/>
							</font>
							<padding>
								<Insets bottom="5.0"
								        left="5.0"
								        right="5.0"
								        top="5.0"/>
							</padding>
						</Label>
					</children>
					<styleClass>
						<String fx:value="user-pane-hover"/>
						<String fx:value="user-pane"/>
					</styleClass>
				</VBox>
			</graphic>
		</Button>
	</children>
	<cursor>
		<Cursor fx:constant="HAND"/>
	</cursor>
</AnchorPane>
