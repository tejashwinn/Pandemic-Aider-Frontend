<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="userBorderPane" prefHeight="700.0" prefWidth="800.0"
            style="visibility: true;" xmlns="http://javafx.com/javafx/16"
            fx:controller="pandemic.aider.client.ui.main.DisplayUserPosts">
    <top>
        <HBox prefHeight="58.0" prefWidth="802.0" style="-fx-background-color: #C7D7B6;" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="userUsernameLabel" contentDisplay="CENTER" prefHeight="58.0" prefWidth="582.0"
                       style="-fx-background-color: #C7D7B6;" text="Username">
                    <font>
                        <Font name="System Bold Italic" size="25.0"/>
                    </font>
                    <HBox.margin>
                        <Insets left="20.0"/>
                    </HBox.margin>
                </Label>
                <Pane prefHeight="58.0" prefWidth="18.0" HBox.hgrow="ALWAYS"/>
                <Button mnemonicParsing="false" onAction="#backActionEvent" prefHeight="40.0" prefWidth="70.0"
                        text="Back">
                    <HBox.margin>
                        <Insets right="20.0" top="5.0"/>
                    </HBox.margin>
                </Button>
            </children>
            <BorderPane.margin>
                <Insets/>
            </BorderPane.margin>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </HBox>
    </top>
    <center>
        <TitledPane fx:id="postViewTitledPane" animated="false" maxHeight="1.7976931348623157E308"
                    maxWidth="1.7976931348623157E308" prefHeight="702.0" prefWidth="802.0" text="Posts"
                    BorderPane.alignment="CENTER">
            <content>
                <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="0.0"
                            minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                    <children>
                        <HBox alignment="CENTER" layoutX="10.6" layoutY="10.6" prefHeight="616.8" prefWidth="800.8"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <children>
                                <Pane prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS"/>
                                <ScrollPane maxWidth="-Infinity" prefHeight="593.0" prefWidth="777.0"
                                            style="-fx-background-color: black;" HBox.hgrow="ALWAYS">
                                    <content>
                                        <GridPane fx:id="userGridPane" alignment="CENTER" prefHeight="200.0"
                                                  prefWidth="760.0" style="-fx-border-color: black;">
                                            <columnConstraints>
                                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                                            </columnConstraints>
                                            <rowConstraints>
                                                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                                            </rowConstraints>
                                        </GridPane>
                                    </content>
                                </ScrollPane>
                                <Pane prefHeight="0.0" prefWidth="0.0" HBox.hgrow="ALWAYS"/>
                            </children>
                        </HBox>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </center>
</BorderPane>
