<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="70.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/16"
            fx:controller="pandemic.aider.client.ui.main.UserPaneController">
    <children>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#viewUser" prefHeight="70.0" prefWidth="760.0">
            <graphic>
                <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="62.0"
                      prefWidth="761.0">
                    <children>
                        <Label fx:id="usernameLabel" maxHeight="1.7976931348623157E308"
                               maxWidth="1.7976931348623157E308" prefHeight="70.0" prefWidth="750.0" text="Name">
                            <font>
                                <Font name="System Bold Italic" size="18.0"/>
                            </font>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </Label>
                        <Label fx:id="nameLabel" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308"
                               maxWidth="1.7976931348623157E308" prefHeight="70.0" prefWidth="760.0" text="Username">
                            <font>
                                <Font name="System Bold Italic" size="18.0"/>
                            </font>
                            <padding>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </padding>
                        </Label>
                    </children>
                </HBox>
            </graphic>
        </Button>
    </children>
</AnchorPane>
